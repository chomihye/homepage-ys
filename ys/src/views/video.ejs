<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link rel="stylesheet" href="css/index.css">
  <link rel="stylesheet" href="css/reset.css">
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
</head>

<body>
  <%- include("part/_header") %>
    <main class="header-padding">
      <div class="thumb-section center">
        <h2 class="text-center ">영상디자인</h2>
        <div class="img-grid">
          <% const items=[ { title: "대전 교육 정보원" , category: "캐릭터" }, { title: "Gplum" , category: "캐릭터" }, {
            title: "대한족구협회" , category: "캐릭터" }, { title: "YBM 레몬" , category: "캐릭터" }, { title: "YBM 시사" ,
            category: "캐릭터" }, { title: "YBM 초등" , category: "캐릭터" }, { title: "YBM 초등" , category: "캐릭터" }, {
            title: "YBM 초등" , category: "캐릭터" }, { title: "경기도 교육청" , category: "캐릭터" }, { title: "경기도 안전 공제회" ,
            category: "캐릭터" }, { title: "대전 교육 정보원" , category: "캐릭터" }, { title: "교학사" , category: "캐릭터" } ]; %>
            <% for (let i=0; i < items.length; i++) { %>
              <div class="img-item">
                <div class="img-item-wrap"><img src="../assets/img/ani/ani<%= i+1 %>.png" alt="<%= items[i].title %>">
                </div>
                <div class="img-title">
                  <%= items[i].title %>
                    <span class="bar-gy"></span>
                    <span class="category">
                      <%= items[i].category %>
                    </span>
                </div>
              </div>
              <% } %>
        </div>
      </div>
      <div class="d-flex jc-center more-btn">
        <button type="button" class="btn-border btn-md gap-12">더보기
          <span class="ic-play"></span>
        </button>
      </div>



      <%- include("part/_ask") %>
    </main>
    <%- include("part/_footer") %>
</body>

</html>

<script src="./js/index.js" type="text/javascript"></script>